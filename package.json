{
  "name": "trilogy-studio-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "prepublishOnly": "npm run build",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm install ./lib -D && vite",
    "local": "node scripts/run_local.js",
    "build": "vue-tsc -b && prettier --write \"./**/*.{js,ts,vue}\" && vite build",
    "build:lib": "pnpm -C lib build",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "test-playwright": "playwright test",
    "test-playwright:ui": "playwright test --ui",
    "format": "prettier --write \"./**/*.{js,ts,vue}\"",
    "benchmark-llm": "npx ts-node scripts/benchmark-llm.ts"
  },
  "devDependencies": {
    "@motherduck/wasm-client": "^0.6.6",
    "@playwright/test": "^1.58.1",
    "@trilogy-data/trilogy-studio-components": "link:lib",
    "@types/gapi": "^0.0.47",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.0",
    "@types/prismjs": "^1.26.5",
    "@types/tabulator-tables": "^6.3.1",
    "@types/webappsec-credential-management": "^0.6.9",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "dompurify": "^3.3.1",
    "jose": "^6.1.3",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "split.js": "^1.6.5",
    "trilogy-studio-components": "link:lib",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vega-lite": "^6.4.2",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^4.0.18",
    "vue": "3.5.27",
    "vue-tsc": "^3.2.4",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.29.1-dev269.0",
